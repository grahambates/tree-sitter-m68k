{
  "name": "tree-sitter-m68k",
  "version": "0.1.1",
  "description": "Motorola 68000 family assembly grammar for tree-sitter",
  "main": "bindings/node",
  "keywords": [
    "parser",
    "lexer",
    "68000",
    "m68k",
    "assembly",
    "asm",
    "amiga"
  ],
  "scripts": {
    "build": "tree-sitter generate",
    "test": "tree-sitter test"
  },
  "author": "Graham Bates",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.15.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.6"
  },
  "repository": "https://github.com/grahambates/tree-sitter-m68k",
  "tree-sitter": [
    {
      "scope": "source.m68k",
      "file-types": [
        "asm",
        "ASM",
        "s",
        "S",
        "i",
        "I"
      ],
      "highlights": "queries/highlights.scm",
      "folds": "queries/folds.scm"
    }
  ]
}
